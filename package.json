{
  "name": "ts-aws-log-lib",
  "version": "0.1.0",
  "description": "A TS metric/log/trace/event library for AWS",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/aws-lambda": "8.10.x",
    "@types/jest": "25.2.x",
    "@types/node": "14.0.x",
    "jest": "26.0.x",
    "prettier": "2.0.x",
    "serverless": "1.71.3",
    "serverless-offline": "6.1.x",
    "serverless-plugin-tracing": "2.0.x",
    "serverless-plugin-typescript": "1.1.x",
    "serverless-webpack": "5.3.x",
    "source-map-support": "0.5.x",
    "ts-jest": "26.0.x",
    "ts-loader": "7.0.x",
    "ts-node": "8.10.x",
    "tslint": "6.1.x",
    "tslint-config-prettier": "1.18.x",
    "typescript": "3.9.x",
    "webpack": "4.43.x",
    "webpack-node-externals": "1.7.x"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "test": "jest --config jestconfig.json",
    "version": "npm run format && git add -A src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rwaterman/ts-lib-starter.git"
  },
  "keywords": [
    "TypeScript",
    "Starter"
  ],
  "author": "Rick Waterman",
  "license": "MIT",
  "dependencies": {
    "aws-xray-sdk": "^3.0.1",
    "clean-stack": "^2.2.0",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.2.1"
  }
}
